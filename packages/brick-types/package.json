{
  "name": "@easyops/brick-types",
  "version": "2.1.0",
  "description": "Pure types for kernel",
  "homepage": "https://git.easyops.local/anyclouds/next-core/tree/master/packages/brick-types",
  "license": "UNLICENSED",
  "main": "dist/index.bundle.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    ".schema"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.easyops.local:anyclouds/next-core.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "start": "concurrently -k -n tsc,build \"npm run start:types\" \"npm run start:rollup\"",
    "start:rollup": "rollup --watch --config rollup.config.js",
    "start:types": "tsc --watch --project tsconfig.types.json",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:rollup",
    "build:rollup": "rollup --config rollup.config.js",
    "build:types": "tsc --project tsconfig.types.json",
    "postbuild": "node scripts/post-build.js",
    "docs:run": "api-extractor run",
    "docs:run:dev": "api-extractor run --local --verbose",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest"
  },
  "sideEffects": false,
  "devDependencies": {
    "typescript-json-schema": "^0.43.0"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "*",
    "antd": "*",
    "history": "*",
    "path-to-regexp": "*"
  }
}
