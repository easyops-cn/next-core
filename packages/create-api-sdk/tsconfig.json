{
  "extends": "../../tsconfig.module.json",
  "compilerOptions": {
    "strictNullChecks": false,
    "moduleResolution": "node16",
    "target": "ES6",
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "dist/",
    "template/",
    "**/__mocks__/",
    "package.json"
  ]
}
