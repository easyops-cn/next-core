<!DOCTYPE html>
<html lang="zh-CN" data-theme="light" data-mode="default">
  <head>
    <meta charset="utf-8">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <base href="<%= htmlWebpackPlugin.options.baseHref %>">
    <link rel="shortcut icon" type="image/png" href="<%= htmlWebpackPlugin.options.faviconPath %>"/>
    <script>(!("documentMode" in document)&&CSS.supports("display","grid")&&window.customElements)||(location.href="<%= htmlWebpackPlugin.options.baseHref %>browse-happy.html")</script>
    <script>window.MOCK_DATE="<%= htmlWebpackPlugin.options.mockDate %>";window.PUBLIC_ROOT=window.CORE_ROOT=window.PUBLIC_CDN="<%= htmlWebpackPlugin.options.publicCdn %>";</script>
    <%= htmlWebpackPlugin
      .tags
      .headTags
      .concat(
        htmlWebpackPlugin
          .tags
          .bodyTags
      )
      .map(htmlWebpackPlugin.options.customizeLinkTag)
      .filter(Boolean)
      .join('')
    %>
  </head>
  <body>
    <div id="root-layout">
      <nav id="menu-bar-mount-point" class="print-hide"></nav>
      <div id="root-content">
        <header id="app-bar-mount-point" class="print-hide"></header>
        <div id="main-mount-point"></div>
        <div id="legacy-iframe-mount-point"></div>
      </div>
      <div id="loading-bar-mount-point"><div class="global-loading-bar initial-loading-bar"><div></div></div></div>
    </div>
    <div id="bg-mount-point"></div>
    <div id="portal-mount-point"></div>
    <%= htmlWebpackPlugin
      .tags
      .headTags
      .concat(
        htmlWebpackPlugin
          .tags
          .bodyTags
      )
      .map(htmlWebpackPlugin.options.customizeScriptTag)
      .filter(Boolean)
      .join('')
    %>
  </body>
</html>
