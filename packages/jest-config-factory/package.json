{
  "name": "@next-core/jest-config-factory",
  "version": "0.4.13",
  "description": "Jest config factory for brick-next related packages",
  "homepage": "https://github.com/easyops-cn/next-core/tree/master/packages/jest-config-factory",
  "license": "GPL-3.0",
  "type": "module",
  "typings": "./index.d.ts",
  "bin": {
    "next-jest": "./bin/next-jest.js"
  },
