{
  "compilerOptions": {
    "declaration": false,
    "sourceMap": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "jsx": "preserve",
    "useUnknownInCatchVariables": false
  }
}
